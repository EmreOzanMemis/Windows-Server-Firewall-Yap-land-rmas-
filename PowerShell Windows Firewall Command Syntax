New-NetFirewallRule
   [-PolicyStore <String>]
   [-GPOSession <String>]
   [-Name <String>]
   -DisplayName <String>
   [-Description <String>]
   [-Group <String>]
   [-Enabled <Enabled>]
   [-Profile <Profile>]
   [-Platform <String[]>]
   [-Direction <Direction>]
   [-Action <Action>]
   [-EdgeTraversalPolicy <EdgeTraversal>]
   [-LooseSourceMapping <Boolean>]
   [-LocalOnlyMapping <Boolean>]
   [-Owner <String>]
   [-LocalAddress <String[]>]
   [-RemoteAddress <String[]>]
   [-Protocol <String>]
   [-LocalPort <String[]>]
   [-RemotePort <String[]>]
   [-IcmpType <String[]>]
   [-DynamicTarget <DynamicTransport>]
   [-Program <String>]
   [-Package <String>]
   [-Service <String>]
   [-InterfaceAlias <WildcardPattern[]>]
   [-InterfaceType <InterfaceType>]
   [-LocalUser <String>]
   [-RemoteUser <String>]
   [-RemoteMachine <String>]
   [-Authentication <Authentication>]
   [-Encryption <Encryption>]
   [-OverrideBlockRules <Boolean>]
   [-CimSession <CimSession[]>]
   [-ThrottleLimit <Int32>]
   [-AsJob]
   [-WhatIf]
   [-Confirm]
   [<CommonParameters>]
